package castinginjava;
class X{
	 int  harshal(int a,int b) {//static method
    int res=a+b;
    System.out.println(res);
	return res;
	}
	class Y extends X {
		 double harshal1(double salary) {//non static method
		System.out.println("non static void method");
			return salary;
		}	
	}
	}

public class Updowncasting {

	public static void main(String[] args) {
    // upcasting = object created childclass(B) reference to parent class(A)3
		A a1=new B();//UPCASTING 
	 
		B b2=new B();
		System.out.println("static method harshal return type: "+X.harshal(40, 50));
		
		
		

	}

}
