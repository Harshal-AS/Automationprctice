package assignments;
interface A{
	void Harshal();

interface B {
	void Harshal2();
		
interface C {
	void Harshal3();
}
class D implements A,B,C{
	public void Harshal1() {
		System.out.println("overriding Harshal 1 method");
	}
		public void Harshal2() {
			System.out.println("overriding Harshal 2 method");
		}
			public void Harshal3() {
				System.out.println("overriding Harshal 3 method");	
				void harshal() {
					
				}
			}
}
}
}


public class Assignment1 {

	public static void main(String[] args) {
		D ref= new D;
		
	}
		
	}

