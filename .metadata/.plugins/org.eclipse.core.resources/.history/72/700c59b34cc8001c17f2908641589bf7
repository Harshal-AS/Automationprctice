package handlingmultipleelements;

import java.util.concurrent.TimeUnit;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class Cricinfowebpage {

	public static void main(String[] args) {
		String driver2=".//executables//chromedriver.exe";
		System.setProperty("WebDriver.chrome.driver",driver2);
		WebDriver cdriver=new ChromeDriver();
		cdriver.manage().window().maximize();
		cdriver.manage().timeouts().implicitlyWait(20, TimeUnit.SECONDS);
		cdriver.get("https://www.cricinfo.com");
		
		
	}

}
