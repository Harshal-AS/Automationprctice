package Logicalprogramjavainterview;

public class Sta {

	public static void main(String[] args) {
		String s="THIS IS MY FIRST INTERVIEW";
		
        String ulhas[]= {"THIS", " IS", " MY", "FIRST", "INTERVIEW"};
        System.out.println(ulhas.length);//4
    //  fixed iteration so, use the for loop
    for(int i=0;i<ulhas.length; i++);
    System.out.println(ulhas[4]);  

	}

