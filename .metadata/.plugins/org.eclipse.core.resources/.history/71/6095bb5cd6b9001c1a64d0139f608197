package Arroys;

public class Singledimensionall {

	public static void main(String[] args) {
		int []d;
		d=new int[4];
// or
	//int []a=new int[45];
		System.out.println(d[0]);// o =null value
		System.out.println(d[1]);// o =null value  Because not initilized the value
		System.out.println(d[2]);// o =null value
		System.out.println(d[3]);// o =null value

	d[0]=20;

}

}
