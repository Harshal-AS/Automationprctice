package unaryoperator;

public class Unaryoperator {

	public static void main(String[] args) {
		int a=34,b;
		b=a++;//b=34,a=35
		System.out.println("a:"+a);//35
		System.out.println("b:"+b);//34
		
		int z=a,x;//z=35
		x=z--;
		System.out.println("z:"+z);//33
		System.out.println("x:"+x);//35
		
	}

}
