package wrapperclass;

public class Boxingoperation {

	public static void main(String[] args) {
	 
		double salary=1234.34;//primative type
		System.out.println("salary: "+salary);//1234.34
		 Double obj=4521.10;//implicit /auto boxing--->  java compiler convert it into Double obj=new Double(4521.10);
		 System.out.println(" autoboxing: "+obj);
		 double obj1=new Double(salary);// explicit boxing
		 System.out.println(" explicit boxing: "+obj1);
	}

}
